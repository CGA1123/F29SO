<%= form_tag user_skills_search_path(id: @user.id), method: :post, remote: true do %>
  <%= select_tag :skill_type_id, options_for_select(SkillType.all.collect{ |u| [u.name, u.id] })%>
  <%= text_field_tag :skill_name, "", placeholder: 'Search For A Skill', autocomplete: :off, onkeyup: "search_skill()" %>
<% end %>
