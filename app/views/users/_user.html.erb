<div class="row z-depth-2" style="display: flex;" id="<%= user.id %>">
  <div class="left" style="margin: 5px 5px 0px 5px;"><%= user_avatar(user) %></div>
  <div class="right" style="display: flex; justify-content: center; align-items: center;">
    <div>
      <p class='name left-align'><%= user.name %></p>
      <p class="grey-text"><%= user.email %></p>
    </div>
  </div>
</div>
