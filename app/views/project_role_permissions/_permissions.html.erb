<% Permission.where("name like 'projects.%'").each do |permission| %>
  <%= check_box_tag 'permission',
                    permission.id,
                    @project_role_permissions.include?(permission),
                    onclick: "updateProjectRolePermission('#{@project.code}','#{@project_role.name}', this);",
                    disabled: @disabled %>
  <%= label_tag permission.name, permission.name %>
<% end %>
