<% if @users.blank? %>
  $('#user-management-table-<%= @project_role.id %> tbody').html("Not Found");
<% else %>
  $('tbody#user-management-table-<%= @project_role.id %>').empty();
  <% @users.each do |user| %>
  $('tbody#user-management-table-<%= @project_role.id %>').append("<%= j render 'project_role_users/user_row', user: user, project_role: @project_role %>");
  <% end %>
<% end %>
